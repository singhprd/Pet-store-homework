<h2>Edit store:</h2>

<form action="/stores/<%= @store.id %>" method="post">

  <label for="name">Name:</label>
  <input type="text" name="name" value="<%= @store.name %>"></input>
  
  <label for="address">address:</label>
  <input type="text" name="address" value="<%= @store.address %>"></input>

  <label for="stock_type">Stock Type:</label>
  <input type="text" name="stock_type" value="<%= @store.stock_type %>"></input>
 

  <input type='submit' value="Update store">
  
</form>













</form>